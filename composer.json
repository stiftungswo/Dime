{
	"name" : "stiftungswo/dimeerp",
	"description" : "DimeERP",
	"keywords" : [
		"timetracker",
		"erp",
		"Symfony2",
		"FOSRestBundle"
	],
	"homepage" : "https://github.com/stiftungswo/Dime",
	"license" : "MIT",
	"authors" : [{
			"name" : "PHPUGL",
			"email" : "info@phpugl.de"
		}, {
			"name" : "StiftungSWO",
			"email" : "dev@stiftungswo.ch"
		}
	],
	"autoload" : {
		"psr-4" : {
			"" : "src/"
		}
	},
	"require" : {
		"php" : ">=7.1",
		"symfony/symfony" : "~2.8",
		"doctrine/orm" : "~2.2,>=2.2.3",
		"doctrine/doctrine-bundle" : "~1.2",
		"twig/extensions" : "~1.0",
		"symfony/assetic-bundle" : "~2.3",
		"symfony/swiftmailer-bundle" : "~2.3",
		"symfony/monolog-bundle" : "~2.4",
		"sensio/distribution-bundle" : "~4.0",
		"sensio/framework-extra-bundle" : "~3.0",
		"sensio/generator-bundle" : "~2.3",
		"incenteev/composer-parameter-handler" : "~2.0",
		"doctrine/doctrine-fixtures-bundle" : "2.3.0",
		"stof/doctrine-extensions-bundle" : "^1.2",
		"jms/serializer-bundle" : "~1.0",
		"friendsofsymfony/rest-bundle" : "~1.7",
		"friendsofsymfony/user-bundle" : "~2.0@dev",
		"nelmio/api-doc-bundle" : "~2.9",
		"knplabs/json-schema-bundle": "1.1.0",
		"misd/phone-number-bundle" : "~1.0",
		"doctrine/migrations": "~1.5",
		"doctrine/doctrine-migrations-bundle": "1.2.1.*@dev",
		"psliwa/pdf-bundle": "~1.0@dev",
		"nesbot/carbon": "^1.22.1",
		"sentry/sentry": "^1.8",
		"sentry/sentry-symfony": "~1.0"
	},
	"require-dev" : {
		"h4cc/alice-fixtures-bundle" : "^0.5.1",
		"phpunit/phpunit": "~4.8",
		"squizlabs/php_codesniffer": "^3.2"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
		"format": "phpcbf --standard=psr2 --ignore=vendor,src/Tbbc/MoneyBundle,src/Money,src/Dime/FrontendBundle/Resources/public --extensions=php src"
	},
	"config" : {
		"bin-dir" : "bin"
	},
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml"
		},
		"branch-alias" : {
			"dev-master" : "2.4-dev"
		}
	}
}
