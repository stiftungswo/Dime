    {% javascripts
        '@DimeTimetrackerFrontendBundle/Resources/public/js/less-1.1.3.min.js' 
        '@DimeTimetrackerFrontendBundle/Resources/public/js/modernizr-2.0.6.min.js' 
    %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <link href="{{ asset('bundles/dimetimetrackerfrontend/images/favicon.ico') }}" rel="shortcut icon">
    <link href="{{ asset('bundles/dimetimetrackerfrontend/images/apple-touch-icon.png') }}" rel="apple-touch-icon">
    <link href="{{ asset('bundles/dimetimetrackerfrontend/images/apple-touch-icon-72x72.png') }}" sizes="72x72" rel="apple-touch-icon">
