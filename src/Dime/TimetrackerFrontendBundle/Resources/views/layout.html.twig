  <body>
    <div class="topbar">
      <div class="fill">
        <div class="container">
          <a class="brand" href="{{ path('_welcome') }}">Dime</a>
          <ul class="nav">
            <li><a href="{{ path('_welcome') }}">Home</a></li>
            <li><a href="{{ path('dime_timetrackerfrontend_customer_index') }}">Customer</a></li>
            <li><a href="{{ path('dime_timetrackerfrontend_project_index') }}">Projects</a></li>
            <li><a href="{{ path('dime_timetrackerfrontend_service_index') }}">Services</a></li>
            <li class="dropdown" data-dropdown="dropdown">
              <a href="{{ path('dime_timetrackerfrontend_help_index') }}" class="dropdown-toggle">Help</a>
              <ul class="dropdown-menu">
                <li><a href="{{ path('dime_timetrackerfrontend_tutorial_index') }}">Tutorial</a></li>
                <li><a href="{{ path('dime_timetrackerfrontend_documentation_index') }}">Documentation</a></li>
                <li><a href="{{ path('dime_timetrackerfrontend_api_index') }}">API</a></li>
              </ul>
            </li>
            <li><a href="#">Settings</a></li>
          </ul>
        </div>
      </div>
    </div>

    <div class="container">
      <div class="content">
          <div class="row">
              <div class="span10">
                  {% block content %}
                    <p>Block: Content</p>
                  {% endblock %}
              </div>
              <div class="span4">
                  {% block sidebar %}
                    <p>Block: Sidebar</p>
                  {% endblock %}
              </div>
          </div>
      </div>

      <footer>
        <p>PHPUGL 2011 | Powered by Symfony2</p>
      </footer>
    </div> <!-- /container -->

    {% javascripts
      '@DimeTimetrackerFrontendBundle/Resources/public/js/jquery-1.7.min.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-dropdown.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-buttons.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-alerts.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-modal.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-twipsy.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/bootstrap-popover.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/jquery.tablesorter.min.js'
      '@DimeTimetrackerFrontendBundle/Resources/public/js/application.js' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
